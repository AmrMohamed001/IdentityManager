@model Role_Identity.Models.ViewModels.VerifyAuthenticatorViewModel

@{
    ViewData["Title"] = "VerifyAuthenticatorCode";
}

<h1>VerifyAuthenticatorCode</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="VerifyAuthenticatorCode" asp-controller="account" asp-route-returnurl="@ViewData["ReturnUrl"]">
            <input asp-for="RememberMe" class="form-control" hidden/>
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Code" class="control-label"></label>
                <input asp-for="Code" class="form-control" />
                <span asp-validation-for="Code" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
